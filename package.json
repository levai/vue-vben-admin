{
  "name": "vue-vben-admin",
  "version": "1.0.0",
  "private": true,
  "description": "Vue Vben Admin 全栈项目 - 前后端统一管理",
  "scripts": {
    "dev": "node scripts/start.js",
    "dev:frontend": "node scripts/start.js frontend",
    "dev:backend": "node scripts/start.js backend",
    "dev:all": "node scripts/start.js all",
    "dev:ele": "cd frontend && pnpm -F @vben/web-ele run dev",
    "dev:play": "cd frontend && pnpm -F @vben/playground run dev",
    "dev:docs": "cd frontend && pnpm -F @vben/docs run dev",
    "install": "bash scripts/install.sh",
    "install:frontend": "bash scripts/install.sh --frontend-only",
    "build": "cd frontend && pnpm build:antd",
    "build:frontend": "cd frontend && pnpm build",
    "clean": "cd frontend && pnpm clean && cd ../backend && mvn clean",
    "lint": "cd frontend && pnpm lint",
    "format": "cd frontend && pnpm format"
  },
  "engines": {
    "node": ">=20.12.0",
    "pnpm": ">=10.0.0"
  }
}
